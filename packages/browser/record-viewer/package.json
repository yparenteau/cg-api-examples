{
    "name": "@activfinancial/record-viewer",
    "description": "A simple record viewer using cg-api.",
    "repository": {
        "type": "git",
        "url": "https://github.com/activfinancial/cg-api-examples"
    },
    "version": "0.6.3",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "CC-BY-4.0",
    "author": {
        "name": "ACTIV Financial Systems, Inc.",
        "email": "support@activfinancial.com",
        "url": "http://www.activfinancial.com"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**"
    ],
    "scripts": {
        "clean": "rimraf lib/*",
        "types": "dts-generator --prefix \"@activfinancial/record-viewer\" --name \"@activfinancial/record-viewer\" --main \"@activfinancial/record-viewer/src/index\" --project . --out lib/index.d.ts",
        "build": "yarn types && webpack --mode production",
        "develop": "yarn types && webpack --mode development",
        "test": "echo \"No test specified\"",
        "watch": "webpack --mode development --watch -d",
        "serve": "webpack-dev-server --mode development -d",
        "prepublishOnly": "yarn build"
    },
    "dependencies": {
        "@activfinancial/cg-api": "^0.9.6",
        "skatejs": "^5.2.4"
    },
    "devDependencies": {
        "@types/skatejs": "^5.0.1",
        "dts-generator": "^3.0.0",
        "raw-loader": "^3.0.0",
        "rimraf": "^2.6.3",
        "source-map-loader": "^0.2.4",
        "terser-webpack-plugin": "^1.3.0",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.2",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.7.2"
    },
    "gitHead": "676979907056fd49e1b275b2c4b494764ca61dbd"
}
